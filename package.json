{
  "name": "clickcs-discordbot",
  "version": "1.0.0",
  "description": "A Discord bot with slash commands",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node --expose-gc --trace-warnings main.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 --trace-warnings main.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --watch",
    "test:debug": "cross-env NODE_OPTIONS=--inspect-brk --experimental-vm-modules jest --config=jest.config.js --runInBand",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "check-updates": "npx npm-check-updates"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@jest/globals": "^29.7.0",
    "@sapphire/ratelimits": "^2.4.11",
    "discord-api-types": "^0.38.0",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.3",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.3",
    "babel-jest": "^30.0.0-beta.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}
