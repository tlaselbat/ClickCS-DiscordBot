{
    "bot": {
        "prefix": "!",
        "version": "1.0.0",
        "maxRetries": 3,
        "retryDelay": 2000
    },
    "permissions": {
        "ownerID": "YOUR_USER_ID_HERE"
    },
    "roles": {
        "voiceChannel": {
            "name": "in vc",
            "color": "#00ff00",
            "mentionable": true
        }
    },
    "logging": {
        "level": "info",
        "file": {
            "maxSize": "5MB",
            "maxFiles": 5
        }
    },
    "database": {
        "enabled": false,
        "type": "sqlite",
        "path": "./data/bot.db"
    }
}